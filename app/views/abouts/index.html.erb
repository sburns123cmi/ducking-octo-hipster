<h1>Listing abouts</h1>

<table>
  <tr>
    <th>Title</th>
    <th>Description</th>
    <th>Producer name</th>
    <th>Producer title</th>
    <th>Producer email</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @abouts.each do |about| %>
  <tr>
    <td><%= about.title %></td>
    <td><%= about.description %></td>
    <td><%= about.producer_name %></td>
    <td><%= about.producer_title %></td>
    <td><%= about.producer_email %></td>
    <td><%= link_to 'Show', about %></td>
    <td><%= link_to 'Edit', edit_about_path(about) %></td>
    <td><%= link_to 'Destroy', about, method: :delete, data: { confirm: 'Are you sure?' } %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New About', new_about_path %>
